{
  "manifest_version": 3,
  "name": "jianding9 extension",
  "version": "0.0.1",
  "description": "插件描述",
  "author": "jianding9",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "host_permissions": [
    "https://*/*"
  ],
  "permissions": [],
  "action": {
    "default_icon": {
      "16": "images/16x16.png",
      "32": "images/32x32.png",
      "64": "images/64x64.png",
      "128": "images/128x128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "64": "images/64x64.png",
    "128": "images/128x128.png"
  }
}